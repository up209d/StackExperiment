{
  "name": "stack-experiment",
  "version": "1.0.0",
  "description": "full stack experiment",
  "main": "dev-server.js",
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js ./dist",
    "env-development": "set NODE_ENV=development",
    "env-production": "set NODE_ENV=production",
    "http-server": "node ./node_modules/http-server-spa/server -p ./index.html 20987",
    "http-server:dist": "node ./node_modules/http-server-spa/server ./dist ./index.html 20987",
    "transform-production-config": "node ./node_modules/babel-cli/bin/babel.js webpack.config.production.js --out-file webpack.config.production.legacy.js",
    "transform-build-server": "node ./node_modules/babel-cli/bin/babel.js ./build-server.js --out-file ./build-server.legacy.js",
    "start": "echo 'Starting ...' && npm run clean && npm run env-development && npm run transform-production-config && npm run transform-build-server && node ./build-server.legacy.js",
    "build": "echo 'Running Production ...' && npm run clean && npm run env-production && ./node_modules/babel-cli/bin/babel-node.js node_modules/webpack/bin/webpack.js --config ./webpack.config.production.js",
    "build:serve": "echo 'Running Production ...' && npm run clean && npm run env-production && ./node_modules/babel-cli/bin/babel-node.js ./node_modules/webpack/bin/webpack.js --config ./webpack.config.production.js && npm run http-server:dist",
    "build:serve:root": "echo 'Running Production ...' && npm run clean && npm run env-production && ./node_modules/babel-cli/bin/babel-node.js ./node_modules/webpack/bin/webpack.js --config ./webpack.config.production.js && npm run http-server",
    "dev": "echo 'Running Development ...' && npm run clean && npm run env-development && NODE_PATH=./src ./node_modules/nodemon/bin/nodemon.js --ignore ./src/ --exec ./node_modules/babel-cli/bin/babel-node.js ./dev-server.js",
    "test": "node_modules/webpack/bin/webpack.js --config webpack.config.production.legacy.js --profile --json > debug.log"
  },
  "author": "UP",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "axios": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.0.0-beta",
    "bundle-loader": "^0.5.5",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.1.0",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "d3": "^4.11.0",
    "eslint": "^4.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server-spa": "^1.2.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "mapbox-gl": "^0.40.1",
    "mobile-detect": "^1.3.7",
    "mongoose": "^5.0.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "opn": "^5.1.0",
    "path": "^0.12.7",
    "port-finder": "^0.12.4",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "precss": "^2.0.0",
    "qs": "^6.5.1",
    "quick-local-ip": "^1.0.7",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-tap-event-plugin": "^3.0.2",
    "react-waypoint": "^7.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "styled-components": "^2.2.0",
